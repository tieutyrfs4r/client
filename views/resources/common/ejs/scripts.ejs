<%-settings.TIDIO_URL%>
<script>
    (function() {
        function onTidioChatApiReady() {

            window.tidioChatApi.hide();
            window.tidioChatApi.on("close", function() {
                window.tidioChatApi.hide();
            });
        }

        if (window.tidioChatApi) {
            window.tidioChatApi.on("ready", onTidioChatApiReady);
        }
        const showSupport = async function() {
            await window.tidioChatApi.show();
            await window.tidioChatApi.open();
        }
        document.querySelector(".support1").addEventListener("click", showSupport);
        document.querySelector(".support2").addEventListener("click", showSupport);
    })();
</script>

